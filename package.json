{
  "name": "testokur-ui",
  "description": "testokur react components",
  "version": "1.0.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/NazmiAltun/testokur-ui.git",
  "author": "Nazmi Altun <nazmialtun@windowslive.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "start-storybook -p 6006",
    "format": "prettier \"./src/**/*.+(js|jsx|css|ts|tsx|md)\" --write",
    "lint": "tsc --noEmit && eslint --fix \"**\" && stylelint \"src/**\"",
    "test": "jest --env=jsdom --passWithNoTests --coverage",
    "test:watch": "jest --watchAll --env=jsdom --passWithNoTests",
    "clean": "rimraf .rts2_cache_cjs .rts2_cache_esm dist public storybook-static *.log stats.*",
    "build": "tsc && tsc --p ./tsconfig.es.json",
    "build-storybook": "rimraf storybook-static && build-storybook",
    "cypress": "cypress open -e DEV=true",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.0",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@peculiar/webcrypto": "^1.0.29",
    "@percy/cypress": "^2.3.1",
    "@storybook/addon-actions": "^6.1.0",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-knobs": "^6.1.0",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^6.0.0",
    "@testing-library/react": "^11.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-react-app": "^10.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.3",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.4",
    "stylelint": "^13.3.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-loader": "^8.0.0",
    "typescript": "^4.0.2",
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "react": "^17.0.0",
    "styled-components": "^5.1.0",
    "testokur-utils": "^1.3.0"
  },
  "resolutions": {
    "@babel/preset-env": "^7.8.0"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "testokur",
    "components",
    "ui",
    "library",
    "component"
  ]
}
